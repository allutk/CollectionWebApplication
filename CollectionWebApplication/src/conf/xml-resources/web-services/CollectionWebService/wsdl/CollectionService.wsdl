				</xs:complexType>
			</xs:element>
			<xs:element name="getCollectionResponse" type="tns:collectionType"/>
			
			<xs:element name="addCollectionRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="token" type="xs:string"/>
						<xs:element name="collectionNo" type="xs:string"/>
